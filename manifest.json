{
  "manifest_version": 2,
  "name": "Learn Issues",
  "description": "Display open issues on the labs",
  "version": "0.1",
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
}