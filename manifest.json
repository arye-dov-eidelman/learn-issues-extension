{
  "manifest_version": 2,
  "name": "Learn Issues",
  "description": "Display open issues on the labs",
  "version": "0.1",

  "content_scripts": [{
    "matches": [
      "https://learn.co/*"
    ],
    "js": ["content.js"]
  }],

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "webNavigation"
  ]
}